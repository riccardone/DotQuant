{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "CorrelationId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    },
    "TenantId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    },
    "Token": {
      "type": "string",
      "minLength": 32,
      "maxLength": 512
    },
    "ExpiryDate": {
      "type": "string",
      "format": "date-time"
    },
    "IsRevoked": {
      "type": "boolean"
    }
  },
  "required": [
    "CorrelationId",
    "Token",
    "ExpiryDate",
    "IsRevoked",
    "TenantId"
  ],
  "additionalProperties": false
}
