{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "CorrelationId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    },
    "Markets": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "minItems": 1
    },
    "TenantId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    }
  },
  "required": [
    "CorrelationId",
    "Markets",
    "TenantId"
  ],
  "additionalProperties": false
}
