{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "CorrelationId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    },
    "BankAccountId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 100
    },
    "BankAccountNumber": {
      "type": "string",
      "minLength": 5,
      "maxLength": 34
    },
    "SortCode": {
      "type": "string",
      "minLength": 6,
      "maxLength": 10
    },
    "IBAN": {
      "type": "string",
      "pattern": "^[A-Z]{2}[0-9A-Z]{13,32}$"
    },
    "BIC": {
      "type": "string",
      "pattern": "^[A-Z]{6}[A-Z0-9]{2,5}$"
    },
    "BankName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100
    },
    "AccountHolderName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100
    },
    "AuthorizationToken": {
      "type": "string",
      "minLength": 10,
      "maxLength": 512
    },
    "LinkingServiceProvider": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100
    },
    "TenantId": {
      "type": "string",
      "minLength": 3,
      "maxLength": 256
    }
  },
  "required": [
    "CorrelationId",
    "BankAccountId",
    "BankAccountNumber",
    "SortCode",
    "IBAN",
    "BIC",
    "BankName",
    "AccountHolderName",
    "AuthorizationToken",
    "LinkingServiceProvider",
    "TenantId"
  ],
  "additionalProperties": false
}
